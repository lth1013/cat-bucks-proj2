<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Cat Bucks</title>
    <link rel="stylesheet" href="style.css">
    <link href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet'>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Dancing+Script&display=swap" rel="stylesheet">
</head>
<body>
    <div data-tg-tour="Welcome to the tour"> for </div>
    <section id="header">
        <a href="#" class="logo">Cat Bucks</a>
        <div>
            <ul id="navbar">
                <li><a href="index.html">Home</a></li>
                <li><a href="#cats">Cats</a></li>
                <li><a href="#toys">Cat Toys</a></li>
                <li><a href="#food">Cat Food</a></li>
                <li><a href="#"><i class='bx bx-cart cart-icon'></i></a></li>
            </ul>
            <div class="cart">
                <h2 class="cart-title">Your Cart</h2>
                <div class="cart-content">
                    
                </div>
                <div class="total">
                    <div class="total-title">Total</div>
                    <div class="total-price">$0</div>
                </div>

                <!-- Buy button -->
                <button type="button" class="btn-buy">Buy Now</button>

                <!-- Cart close -->
                <i class='bx bxs-x-circle' id="close-cart" ></i>
            </div>
        </div>  
    </section>


    <!-- Cats For Sale -->

    {{!-- HANDLEBARS OUTLINE --}}
    <div id="cats"> <p>z</p> </div>
    <section class="section-p1 product1">
        <h2>Cat Inventory</h2>
        <p>Cute cats in need of a home</p>
        {{#each products as |product| }}
        <div class="pro-container">
            <div class="pro">
                <img src={{product.image}} alt="Birman cat" class="product-img">
                <div class="des">
                    <span class="product-title">{{product.product_name}}</span>
                    <h5>{{product.description}}</h5>
                    <h4 class="price">{{product.price}}</h4>
                </div>
                <a href="#"><i class='bx bx-cart add-cart'></i></a>
            </div>
        </div>
        {{/each}}

    <!-- Cat Toys for Sale -->
    <div id="toys"> <p>z</p> </div>
    <section class="section-p1 product1">
        <h2>Cat Toys</h2>
        <p>Keep your Cat Entertained</p>
        <div class="pro-container">
            <div class="pro">
                <img src="images/cat-toys/chew-toy.jpg" alt="Chew Toy" class="product-img">
                <div class="des">
                    <span class="product-title">Chew Toy Mouse</span>
                    <h5>Interactive Small Mouse</h5>
                    <h4 class="price">$50</h4>
                </div>
                <a href="#"><i class='bx bx-cart add-cart'></i></a>
            </div>
            <div class="pro">
                <img src="images/cat-toys/laser.jpg" alt="Cat Laser" class="product-img">
                <div class="des">
                    <span class="product-title">Interactive Cat Laser</span>
                    <h5>Pointer 3 in 1</h5>
                    <h4 class="price">$50</h4>
                </div>
                <a href="#"><i class='bx bx-cart add-cart'></i></a>
            </div>
            <div class="pro">
                <img src="images/cat-toys/plush-ball.jpg" alt="Cat Plush ball" class="product-img">
                <div class="des">
                    <span class="product-title">Plush Ball</span>
                    <h5>Multicolor Chew Toy</h5>
                    <h4 class="price">$50</h4>
                </div>
                <a href="#"><i class='bx bx-cart add-cart'></i></a>
            </div>
            <div class="pro">
                <img src="images/cat-toys/plush-fish.jpg" alt="Cat Plush Fish" class="product-img">
                <div class="des">
                    <span class="product-title">Plush Fish Pet</span>
                    <h5>Reative 3D Fish Shape</h5>
                    <h4 class="price">$50</h4>
                </div>
                <a href="#"><i class='bx bx-cart add-cart'></i></a>
            </div>
            <div class="pro">
                <img src="images/cat-toys/plush-fruit.jpg" alt="Cat Plush Fruit" class="product-img">
                <div class="des">
                    <span class="product-title">Fruit Shaped Plush</span>
                    <h5>Bite-Resistant</h5>
                    <h4 class="price">$50</h4>
                </div>
                <a href="#"><i class='bx bx-cart add-cart'></i></a>
            </div>
            <div class="pro">
                <img src="images/cat-toys/plush.jpg" alt="Plush balls" class="product-img">
                <div class="des">
                    <span class="product-title">Polyester Plush Ball</span>
                    <h5>Multicolor Plushes</h5>
                    <h4 class="price">$50</h4>
                </div>
                <a href="#"><i class='bx bx-cart add-cart'></i></a>
            </div>
            <div class="pro">
                <img src="images/cat-toys/squish.jpg" alt="Squishy Panda" class="product-img">
                <div class="des">
                    <span class="product-title">Squishy Panda Buns</span>
                    <h5>Stress Reliever Ball</h5>
                    <h4 class="price">$50</h4>
                </div>
                <a href="#"><i class='bx bx-cart add-cart'></i></a>
            </div>
            <div class="pro">
                <img src="images/cat-toys/teaser.jpg" alt="Feather Teaser" class="product-img">
                <div class="des">
                    <span class="product-title">Feather Teaser</span>
                    <h5>Interactive Bell Toy</h5>
                    <h4 class="price">$50</h4>
                </div>
                <a href="#"><i class='bx bx-cart add-cart'></i></a>
            </div>
        </div>
    <section>
    

    <!-- Cat Food for Sale -->
    <div id="food"> <p>z</p> </div>
    <section class="section-p1 product1">
        <h2>Cat Food</h2>
        <p>Only the best nutrition for your Cat</p>
        <div class="pro-container">
            <div class="pro">
